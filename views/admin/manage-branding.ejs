<%- include('./partials/header') %>
        <!-- ============================================================== -->
        <!-- Start right Content here -->
        <!-- ============================================================== -->
        <div class="main-content">

            <div class="page-content">
                <div class="container-fluid">

                    <!-- start page title -->
                    <div class="row">
                        <div class="col-12">
                            <div class="page-title-box d-flex align-items-center justify-content-between">
                                <h4 class="mb-0 font-size-18">Manage Branding</h4>

                                <div class="page-title-right">
                                    <ol class="breadcrumb m-0">
                                        <li class="breadcrumb-item"><a href="javascript: void(0);">Branding</a></li>
                                        <li class="breadcrumb-item active">Manage Branding</li>
                                    </ol>
                                </div>
                                
                            </div>
                        </div>
                    </div>     
                    <!-- end page title -->

                    <div class="row">
                        <div class="col-12">
                            <div class="card">
                                <div class="card-body">
                                    <h4 class="card-title">Branding</h4>
                                    <p class="card-subtitle mb-4">Here is the lists of the all Enquiries From the website</p>
                                    <div class="table-responsive">
                                        <table id="basic-datatable" class="table">
                                            <thead>
                                                <tr>
                                                    <th>Title</th>
                                                    <th>Banner Image</th>
                                                    <th>About project</th>
                                                    <!-- <th>Images/Videos</th> -->
                                                    <th style="min-width: 100px;">Actions</th>
                                                </tr>
                                            </thead>
                                        
                                        
                                            <tbody>
                                                <% data.forEach(function(data,key) { %>
                                                <tr>
                                                    <td><%= data.title %></td>
                                                    <td><img src="/images/branding/<%= data.banner %>" alt="" height="50"></td>
                                                    <td><%= data.about %></td>
                                                    <td>
                                                        <a href="/admin/edit-branding/<%= data._id %>"><button class="btn btn-primary btn-sm waves-effect waves-light">Edit</button></a>
                                                        <a href="/admin/delete-branding/<%= data._id %>"><button class="btn btn-sm btn-danger waves-effect waves-light">delete</button></a>
                                                    </td>
                                                </tr>
                                                <% }); %>
                                            </tbody>
                                        </table>
                                    </div>

                                </div> <!-- end card body-->
                            </div> <!-- end card -->
                        </div><!-- end col-->
                    </div>

                </div> <!-- container-fluid -->
            </div>
            <!-- End Page-content -->
<%- include('./partials/footer') %>